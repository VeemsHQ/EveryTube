{
    "name": "chrome-cookies-test",
    "description": "chrome-cookies-test desc",
    "version": "0.1.0",
    "icons": {
        "128": "images/icon-128.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/feed/subscriptions*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [ "cookies","storage", "tabs", "http://*/*", "https://*/*" , "nativeMessaging"],
    "manifest_version": 2
}
